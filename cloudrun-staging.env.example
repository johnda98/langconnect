# Fill this file with staging values, then copy to cloudrun-staging.env (gitignored).
# Intended for the Cloud Run staging/early-prod service.
#
# API / vector store
POSTGRES_HOST=/cloudsql/your-vehicle-ai-staging:us-central1:langconnect-staging-sql
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=change-me
POSTGRES_DB=langconnect_staging

# App integrations
OPENAI_API_KEY=<copy-from-.env.staging>
SUPABASE_URL=<copy-from-.env.staging>
SUPABASE_KEY=<copy-from-.env.staging>

# CORS origins for hosted webapp + local dev
ALLOW_ORIGINS=["https://your-staging-frontend.example.com","http://localhost:3000"]
